    memory: 2048
  3.4.3:
    default: true
    command: /usr/bin/octave
'sleep':
  global:
    memory: 2048
  '1.0':
    default: true
    command: /bin/sleep
    args:
      - 9000000s
shell:
  global:
    memory: 2048
  2.31.3:
    default: true 
    command: /usr/bin/gnome-terminal
    command_nogui: $ENV(SHELL)
python:
  global:
    memory: 2048
  '2.7':
    default: true
    command: /usr/bin/gnome-terminal
    command_nogui: /usr/local/bin/python27
    args:
      - -e /usr/local/bin/python27
  '3.3':
    command: /usr/bin/gnome-terminal
    command_nogui: /usr/local/bin/python33
    args:
      - -e /usr/local/bin/python33
