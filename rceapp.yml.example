xstata-mp:
  global:
    memory: 4096
  13.1:
    default: true
    command: /nfs/tools/apps/stata/13.1/xstata-mp
xstata-se:
  global:
    memory: 2048
  13.1:
    default: true
    command: /nfs/tools/apps/stata/13.1/xstata-se
R:
  global:
    memory: 2048
  3.1:
    default: true
    command: /usr/bin/gnome-terminal
    args: 
      - -e /usr/bin/R
SAS:
  global:
    memory: 2048
    args:
